{
  "openapi": "3.0.4",
  "info": {
    "title": "API Logueo y Registro",
    "description": "API Trabajo Pr√°ctico Programacion UTN",
    "contact": {
      "name": "Ignacio Blanco",
      "email": "ignacioeblanco@gmail.com"
    },
    "version": "v1"
  },
  "paths": {
    "/api/ControlDeAuth/register": {
      "post": {
        "tags": [
          "ControlDeAuth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Registro"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Registro"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Registro"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/ControlDeAuth/login": {
      "post": {
        "tags": [
          "ControlDeAuth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/User/profile": {
      "get": {
        "tags": [
          "User"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Login": {
        "type": "object",
        "properties": {
          "mail": {
            "type": "string",
            "nullable": true
          },
          "contrasenia": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Registro": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "mail": {
            "type": "string",
            "nullable": true
          },
          "contrasenia": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Introduce tu token JWT en el siguiente formato: **Bearer {tu_token}**",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}